{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
    "extends": [
      "zavatta"
    ],
    "rules": {
      "no-undef": "warn",
      "no-unused-vars": "warn",
      "no-inline-comments": "off",
      "no-console": "off",
      "id-length": "off",
      "indent": [
        "error",
        2
      ],
      "max-len": [
        "warn",
        {
          "code": 100
        }
      ],
      "no-sync": 0,
      "camelcase": 0
    }
  }
